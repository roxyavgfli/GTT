<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="main\dbBundle\Entity\Imputation" table="imputation">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ttaskId" type="integer" column="ttask_id" nullable="false"/>
    <field name="date" type="date" column="date" nullable="false"/>
    <field name="tempsPasse" type="integer" column="temps_passe" nullable="false"/>
    <field name="raf" type="integer" column="raf" nullable="false"/>
    <field name="commentaire" type="string" column="commentaire" length="500" nullable="false"/>
  </entity>
</doctrine-mapping>
