                                            {%elseif tacheedit is defined and dup is defined%}
                                            <h4 class="modal-title" id="myModalLabel">Cloning report : {%if tacheedit.date is defined%}{%if tacheedit.date is not empty%}{{tacheedit.date}}{%endif%}{%endif%} : {%if tacheedit.nom is defined%}{%if tacheedit.nom is not empty%}{{tacheedit.nom}}{%endif%}{%endif%} {%if tacheedit.nature is defined%}{%if tacheedit.nature is not empty%}{{tacheedit.nature}}{%endif%}{%endif%}<input readonly type="text" id="bookIsd" value="" style="border: none; width : 100%;"/> </h4>
                                            {%endif%}
                                        <input readonly hidden type="text" name="idToEdit" id="bookIsd2" value="" style="border: none; width : 100%;"/>
                                        {% if oldprodid is defined and oldprod is not null %}<input readonly hidden type="text" name="idoldprod" id="bookIsd2" value="{{oldprodid}}" style="border: none; width : 100%;"/>{% endif %}
                                        {% if oldversid is defined and oldversid is not null %}<input readonly hidden type="text" name="idoldvers" id="bookIsd2" value="{{oldversid}}" style="border: none; width : 100%;"/>{% endif %}
                                        {% if oldcompid is defined and oldcompid is not null %}<input readonly hidden type="text" name="idoldcomp" id="bookIsd2" value="{{oldcompid}}" style="border: none; width : 100%;"/>{% endif %}
                                        {% if oldplatid is defined and oldplatid is not null %}<input readonly hidden type="text" name="idoldplat" id="bookIsd2" value="{{oldplatid}}" style="border: none; width : 100%;"/>{% endif %}
                                    </div>
