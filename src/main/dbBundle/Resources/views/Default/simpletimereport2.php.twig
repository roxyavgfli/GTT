                    <input  name="endate" type="text" placeholder="select end date" {%if endatesearch is defined%}{%if endatesearch == '9999/12/30'%}{%else%}value="{{endatesearch}}"{%endif%}{%endif%} id="example2" style="font-size: 15px; width : auto;">
                    <select class="form-control" name="natureshearched">
                        {%if natures is defined%}
