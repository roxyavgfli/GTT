                                        <div class='form-group'>
                                            <label for='InputPassword3' class='col-sm-2 control-label'>Date : </label>
                                            <div class='col-sm-10'>
                                                <input readonly type='text' placeholder='select date' name='dateeditproduit' id='dateeditproduit' value='{{tacheedit.date}}' style='font-size: 15px; width : auto; required'>
                                            </div>
