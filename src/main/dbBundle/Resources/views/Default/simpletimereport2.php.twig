                    <strong style="text-align: center;">{{message}}</strong>
                </div>
            {%endif%}

            <div class="wrap" style="height: 60% !important; border: 2px solid; border-color: #e7e7e7;border-radius: 5px;
                 display: block;
                 box-shadow: 10px 10px 5px #888888;">
                <table class="table table-condensed" style="border-collapse:collapse; margin-bottom: 0px;">
                    <tr>
                        <th style="width:33%;">&nbsp;</th>
                        <th style="width:33%;">Date</th>
                        <th style="width:34%;">Time Reported</th>
                    </tr>
                </table>
                <div class="span3" style="table-layout: fixed; height:90%; overflow-y: auto;">
                    <table class="table table-condensed" style="border-collapse:collapse;">
                        {%if tachestodisplay is defined%}
                            {% set i = 0 %}
                            {%for task in tachestodisplay|reverse%}
                                {%if task[1] != 1%}
                                    {%if task[3] is defined and task[3] == "WE"%}
                                        <tr class="accordion-toggle" style="background-color: grey;"> 
                                        {%elseif task[1] == 0 %}
                                        <tr data-toggle="accordion-toggle" data-target="#demo{{i}}" class="accordion-toggle" style="background-color: red;">
                                        {%else%}
                                        <tr data-toggle="collapse" data-target="#demo{{i}}" class="accordion-toggle" style="background-color: red;">
                                        {%endif%}
                                    {%else%}
                                    <tr data-toggle="collapse" data-target="#demo{{i}}" class="accordion-toggle">
                                    {%endif%}
                                    {%if task[3] is defined and task[3] == "WE"%}
                                        <td style="width:33%;"></td>

                                    {%else%}
                                        {%if task[1] != 1%}
                                            {%if task[1] == 0%}
                                                <td style="width:33%;"><img src="{{asset('bundles/maindb/images/warning.gif')}}" alt="Smiley face" height="20" ><a class="open-AddBookDialog btn btn-default btn-xs" data-id="{{task[0]}}"  href="#AddBookDialog2" ><span class="glyphicon glyphicon-plus"></span></a></td>
                                                    {%else%}
                                                <td style="width:33%;"><img src="{{asset('bundles/maindb/images/warning.gif')}}" alt="Smiley face" height="20" ><a class="open-AddBookDialog btn btn-default btn-xs" data-id="{{task[0]}}"  href="#AddBookDialog2" ><span class="glyphicon glyphicon-plus"></span></a><button class="btn btn-default btn-xs" style="margin-left:5px;"><span class="glyphicon glyphicon-eye-open"></span></button></td>
                                                    {%endif%}
                                                {%else%}
                                            <td style="width:33%;"><button class="btn btn-default btn-xs"><span class="glyphicon glyphicon-eye-open"></span></button></td>
                                                {%endif%}
                                            {%endif%}

                                    <td>{{task[0]}}</td>
                                    {%if task[3] is defined and task[3] == "WE"%}
                                        <td style="width:34%;">Week End</td>
                                    {%else%}
                                        <td style="width:34%;">{{task[1]}}</td>
                                    {%endif%}
                                </tr>
                                <tr>
                                    <td colspan="12" class="hiddenRow">
                                        <div class="accordian-body collapse" id="demo{{i}}"> 
                                            <table class="table table-striped">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 110px;">Nature</th>
                                                        <th style="width: 350px;">Title</th>
                                                        <th style="width: 250px;">Activity</th>
                                                        <th style="width: 80px;"><span class="glyphicon glyphicon-time" style="margin-top: 9px; color: black;"></span></th>
                                                        <th style="width: 80px;">Edit</th>
                                                        <th style="width: 80px;">Delete</th>
                                                        <th style="width: 80px;">Clone</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    {%for ta in task[2]%}
                                                        <tr>
                                                            <td>{{ta.nature}}</td>
                                                            <td>{{ta.nom}}</td>
                                                            <td>
                                                                {%for activite in activites2%}
                                                                    {%if activite.id == ta.activiteId%}
                                                                        {{activite.nom}}
                                                                    {%endif%}
                                                                {%endfor%}
                                                            </td>
                                                            <td>{{ta.tempsPasse}}</td>
                                                            <td>
                                                                {%if ta.editable == 1%}
                                                                    <a title="Edit this item" href="{{ path('maindb_editsimplereport', {'idToEdit': ta.id}) }}"><span class="glyphicon glyphicon-edit" style="margin-right: 10px; margin-left: 10px; margin-top: 8px;"></span></a>
                                                                    {%else%}
                                                                    <a><span class="glyphicon glyphicon-edit" style="margin-right: 10px; margin-left: 10px; margin-top: 9px; color: black;"></span></a>
                                                                    {%endif%}
                                                            </td>      
                                                            <td>
                                                                {%if ta.editable == 1%}
                                                                    <a data-id="{{ta.nom}}" data-id2="{{ta.id}}" title="Delete this item" class="open-AddBookDialog btn" href="#addBookDialog"><span class="glyphicon glyphicon-remove"></span></a>
                                                                    {%else%}
                                                                    <a><span class="glyphicon glyphicon-remove" style="margin-right: 10px; margin-left: 13px; margin-top: 9px; color: black;"></span></a>
                                                                    {%endif%}
                                                            </td> 
                                                            <td>
                                                                <a title="Edit this item" href="{{ path('maindb_duplicatesimplereport', {'idToEdit': ta.id}) }}"><span class="glyphicon glyphicon-plus" style="margin-right: 10px; margin-left: 10px; margin-top: 8px;"></span></a>
                                                            </td>
                                                        </tr>
                                                    {%endfor%}
                                                </tbody>
                                            </table>
                                        </div> 
                                    </td>
                                </tr>
                                {% set i = i+1 %}    
                            {%endfor%}
                        {%endif%}
                    </table>
                </div>   
            </div>
            <div class="modal fade" id="AddBookDialog2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog" style="width: 40%;">
                    <form class="form-horizontal" role="form" data-validate="parsley" method="POST" action="{{path('maindb_newsimplereport')}}">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                <h4 class="modal-title" id="myModalLabel">New report : <input readonly type="text" id="bookIsd" value="" style="border: none; width : 100%;"/> </h4>
                                <input readonly hidden type="text" name="idToEdit" id="bookIsd2" value="" style="border: none; width : 100%;"/>
                            </div>
                            <div class="modal-body">
                                <div style="margin-top: 30px;">

                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Nature : </label>
                                        <div class="col-sm-10">
                                            <select required class="form-control" name="nature" id="nature" onchange="test()">
                                                {%if natures is defined%}
                                                    <option  value="">-- Select nature --</option>
                                                    {%for nature in natures%}
                                                        <option  value="{{nature}}">{{nature}}</option>
                                                    {%endfor%}
                                                {%endif%}
                                            </select>
                                        </div>
                                    </div>
                                    <div class='form-group'>
                                        <label for='InputPassword3' class='col-sm-2 control-label'>Title : </label>
                                        <div class='col-sm-10'>
                                            <input class='form-control parsley-validated' type='text' name='fname' required>
                                            <br>
                                            </input>
                                        </div>
                                    </div>
                                    <div class='form-group'>
                                        <label for='InputPassword3' class='col-sm-2 control-label'>Date : </label>
                                        <div class='col-sm-10'>
                                            <input readonly required type='text' value="" name='date' id='date' style='font-size: 15px; width : auto; required'>
                                        </div>
                                    </div>
                                    <div id="lsld">

                                    </div>
                                    <div id="demo">

                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                                <button type="submit" id="submit" class="btn btn-primary">Next</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>


            <div class="modal fade" id="modal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                <div class="modal-dialog" style="width: 60%;">
                    <form class="form-horizontal" role="form" data-validate="parsley" method="POST" action="{{path('maindb_savenewsimplereport')}}">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                                <h4 class="modal-title" id="myModalLabel">New report : {%if date is defined%}{%if date is not empty%}{{date}}{%endif%}{%endif%} : {%if label is defined%}{%if label is not empty%}{{label}}{%endif%}{%endif%} {%if natureselected is defined%}{%if natureselected is not empty%}{{natureselected}}{%endif%}{%endif%}<input readonly type="text" id="bookIsd" value="" style="border: none; width : 100%;"/> </h4>
                                <input readonly hidden type="text" name="date" id="bookIsd2" value="{%if date is defined%}{%if date is not empty%}{{date}}{%endif%}{%endif%}" style="border: none; width : 100%;"/>
                                <input readonly hidden type="text" name="label" id="bookIsd2" value="{%if label is defined%}{%if label is not empty%}{{label}}{%endif%}{%endif%}" style="border: none; width : 100%;"/>
                                <input readonly hidden type="text" name="naturetosave" id="bookIsd2" value="{%if natureselected is defined%}{%if natureselected is not empty%}{{natureselected}}{%endif%}{%endif%}" style="border: none; width : 100%;"/>
                                <div class="form-group" style="display: none;">
                                    <label for="inputPassword3" class="col-sm-2 control-label">Final Customer : </label>
                                    <div class="col-sm-10">
                                        <select required class="form-control" name="finalcustomer" id="finalcustomer">
                                            {%if clients is defined%}
                                                <option  value="">-- Select Custommer --</option>
                                                {%for client in clients%}
                                                    {%if client.nom == 'V3D' or client.nom == 'v3d'%}
                                                        <option selected value="{{client.id}}">{{client.nom}}</option>
                                                    {%else%}
                                                        <option value="{{client.id}}">{{client.nom}}</option>
                                                    {%endif%}    
                                                {%endfor%}
                                            {%endif%}
                                        </select>
                                    </div>
                                </div>
                            </div>

                            <div class="modal-body">
                                <div style="margin-top: 30px;">

                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Activité : </label>
                                        <div class="col-sm-10">
                                            <select required class="form-control" name="activite" id="activite">
                                                {%if activites is defined%}
                                                    <option  value="">-- Select activite --</option>
                                                    {%for activite in activites%}
                                                        {%if activite.nom == 'produit' or activite.nom == 'Produit' or activite.nom == 'product' or activite.nom == 'Product' %}
                                                            <option selected value="{{activite.id}}">{{activite.nom}}</option>
                                                        {%else%}
                                                            <option value="{{activite.id}}">{{activite.nom}}</option>
                                                        {%endif%}
                                                    {%endfor%}
                                                {%endif%}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Time spent : </label>
                                        <div class="col-sm-10">
                                            <select required class="form-control" name="temps" id="temps">
                                                {%if times is defined%}
                                                    <option  value="">-- Select Time Spent --</option>
                                                    {%for time in times%}
                                                        {%if time == '0.25' %}
                                                            <option  value="{{time}}">1/4 day</option>
                                                        {%elseif time == '0.5' %}
                                                            <option  value="{{time}}">1/2 day</option>
                                                        {%elseif time == '0.75' %}
                                                            <option  value="{{time}}">3/4 day</option>
                                                        {%elseif time == '1.0' %}
                                                            <option  value="{{time}}">Full day</option>
                                                        {%else%}
                                                            <option  value="{{time}}">{{time}}</option>
                                                        {%endif%}
                                                    {%endfor%}
                                                {%endif%}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Product : </label>
                                        <div class="col-sm-10">
                                            <select required class="form-control" name="product" id="productnormal"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Versions : </label>
                                        <div class="col-sm-10">                                   
                                            <select  class="form-control" name="version" id="versionsnormal"></select>  
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Plateforme : </label>
                                        <div class="col-sm-10">
                                            <select  class="form-control" name="plateforme" id="plateformesnormal"></select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Component : </label>
                                        <div class="col-sm-10">
                                            <select  class="form-control" name="component" id="composantsnormal"></select> 
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label for="inputPassword3" class="col-sm-2 control-label">Comment : </label>
                                        <div class="col-sm-10">
                                            <textarea class="form-control" rows="3" name="comment"></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                                <button type="submit" id="submit" class="btn btn-primary">Save</button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>

