<?php

namespace main\dbBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * PhaseSsphaseActivite
 */
class PhaseSsphaseActivite
{
    /**
     * @var integer
     */
    private $phaseId;

    /**
     * @var integer
     */
    private $ssphaseId;

    /**
     * @var integer
     */
    private $activiteId;

    /**
     * @var integer
     */
    private $id;


    /**
     * Set phaseId
     *
     * @param integer $phaseId
     * @return PhaseSsphaseActivite
     */
    public function setPhaseId($phaseId)
    {
        $this->phaseId = $phaseId;

        return $this;
    }

    /**
     * Get phaseId
     *
     * @return integer 
     */
    public function getPhaseId()
    {
        return $this->phaseId;
    }

    /**
     * Set ssphaseId
     *
     * @param integer $ssphaseId
     * @return PhaseSsphaseActivite
     */
    public function setSsphaseId($ssphaseId)
    {
        $this->ssphaseId = $ssphaseId;

        return $this;
    }

    /**
     * Get ssphaseId
     *
     * @return integer 
     */
    public function getSsphaseId()
    {
        return $this->ssphaseId;
    }

    /**
     * Set activiteId
     *
     * @param integer $activiteId
     * @return PhaseSsphaseActivite
     */
    public function setActiviteId($activiteId)
    {
        $this->activiteId = $activiteId;

        return $this;
    }

    /**
     * Get activiteId
     *
     * @return integer 
     */
    public function getActiviteId()
    {
        return $this->activiteId;
    }

    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }
}
